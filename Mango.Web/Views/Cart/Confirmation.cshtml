@model int

<div class="text-center">
	<h1 class="text-info pt-4">Congratulations! You are one step closer to amazing food.</h1>
	<p class="pb-3">We have received your order and will be ready in 30 minutes during regular business hours!</p>
	<h4 class="text-warning pb-3">Order ID - @Model</h4>
	<img src="/images/order.PNG" width="80%"/>
</div>

<div class="card-header bg-dark text-light ml-0 row ">
	<div class="col-11 text-end">
		<a asp-action="Index" asp-controller="Home" class="btn btn-outline-warning mt-2 btn-sm">Continue Shopping</a>
	</div>
</div>